{
  "name": "fitpass-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && prisma generate",
    "start": "node dist/server.js",
    "vercel-build": "npm run build",
    "db:switch": "node scripts/switch-db.js",
    "db:local": "npm run db:switch local && npm run prisma:push",
    "db:cloud": "npm run db:switch cloud && npm run prisma:push",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts",
    "db:seed": "npm run prisma:push && npm run seed"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@types/socket.io": "^3.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "prisma": "^5.19.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.11.24",
    "@vercel/node": "^4.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "18.x"
  }
}
